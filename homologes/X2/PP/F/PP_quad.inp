&GLOBAL
RUN_TYPE ENERGY
PROJECT_NAME F2_PP_quad 
&END GLOBAL
&FORCE_EVAL

&PROPERTIES
        &TDDFPT
                NSTATES 10
                MAX_ITER 20
                MAX_KV 60
                CONVERGENCE 1.0E-8
                &DIPOLE_MOMENTS
                        DIPOLE_FORM LENGTH
                &END DIPOLE_MOMENTS
                KERNEL FULL
                &XC
                   &XC_FUNCTIONAL PBE0
                   &END XC_FUNCTIONAL
                &END XC
                &SOC
                        EPS_FILTER 1.0E-30
                &END SOC
                &PRINT
                        &SOC_PRINT
                                SPLITTING
                                SOME
                        &END SOC_PRINT
                &END PRINT
        &END TDDFPT
&END PROPERTIES
&DFT
        BASIS_SET_FILE_NAME  ../../../basis_quad
        POTENTIAL_FILE_NAME  GTH_SOC_POTENTIALS
        &SCF
                 EPS_SCF 1.0E-7
                 MAX_SCF 20
         &OUTER_SCF
            MAX_SCF 50
            EPS_SCF 1.0E-7
         &END OUTER_SCF
         &OT
                  PRECONDITIONER FULL_ALL
                  MINIMIZER DIIS
         &END OT
   &END SCF
        &MGRID
                CUTOFF 1200
                REL_CUTOFF 80
                NGRIDS 5
        &END
        &QS
                EPS_PGF_ORB 1.0E-30
                EPS_DEFAULT 1.0E-15
                METHOD GPW
        &END QS

        &POISSON
                PERIODIC NONE
                PSOLVER MT
        &END POISSON

        &XC
                &XC_FUNCTIONAL PBE0
                &END XC_FUNCTIONAL
        &END XC
&END DFT
&SUBSYS
        &CELL
                ABC 20.0 20.0 20.0
                PERIODIC NONE
        &END CELL
        &TOPOLOGY
                COORD_FILE_FORMAT XYZ
                COORD_FILE_NAME opt.xyz
                &CENTER_COORDINATES
                &END CENTER_COORDINATES
        &END TOPOLOGY
	@include ../../../kindef_quad
&END SUBSYS
&END FORCE_EVAL
                 
